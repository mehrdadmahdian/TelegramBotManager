    "homepage": "https://github.com/mehrdadmahdian/telegrambotmanger",
    "keywords": ["Laravel", "TelegramBotManger"],
    "require": {
        "illuminate/support": "~5",
        "telegram-bot/api": "2.3.13"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "MehrdadMahdian\\TelegramBotManger\\": "src/"
        }
