            "MehrdadMahdian\\TelegramBotManger\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MehrdadMahdian\\TelegramBotManger\\TelegramBotMangerServiceProvider"
            ],
            "aliases": {
                "TelegramBotManger": "MehrdadMahdian\\TelegramBotManger\\Facades\\TelegramBotManger"
            }
        }
    }
